{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run CLI",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "cmd/cli/main.go",
            "envFile": "${workspaceFolder}/.env",
            "showLog": true
        },
        {
            "name": "Sync CLI",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "cmd/cli/main.go",
            "args": [
                "sync"
            ],
            "envFile": "${workspaceFolder}/.env",
            "showLog": true
        },
    ]
}